            align-items: center;
        }

        .flash .i,
        .flash2 .i {
            font-size: 2rem;
            color: white;
            float: left;
            margin-right: 20px;
        }

        .flash2 {
            background-color: rgb(0, 172, 0);
        }

        @media screen and (max-width: 900px) {

            .flash,
            .flash2 {
                font-size: 0.9rem;
                padding: 12px 25px;
                margin: 3px;
                left: 0;
                right: 0;
            }

            .flash .i,
            .flash2 .i {
                font-size: 1.5rem;
            }
        }
    </style>

